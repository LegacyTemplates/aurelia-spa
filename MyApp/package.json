{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack --config webpack.config.js --watch",
    "dev-server": "webpack-dev-server",
    "dtos": "cd src && typescript-ref",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod && dotnet publish -c Release",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test.fileMock.js"
    }
  },
  "dependencies": {
    "@servicestack/client": "^1.0.14",
    "aurelia-animator-css": "^1.0.4",
    "aurelia-bootstrapper": "^2.2.0",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-framework": "^1.3.0",
    "aurelia-history-browser": "^1.2.0",
    "aurelia-loader-webpack": "^2.2.1",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal-browser": "^1.8.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-router": "^1.6.1",
    "aurelia-templating": "^1.8.1",
    "aurelia-templating-binding": "^1.4.3",
    "aurelia-templating-resources": "^1.7.0",
    "aurelia-templating-router": "^1.3.1",
    "bootstrap": "^4.1.1",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@servicestack/cli": "^1.0.5",
    "@types/jest": "^23.1.3",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "aurelia-webpack-plugin": "^2.0.0-rc.5",
    "autoprefixer": "^8.6.4",
    "awesome-typescript-loader": "^4.0.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "precss": "^3.1.2",
    "raw-loader": "^0.5.1",
    "run-sequence": "^2.2.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^2.11.2"
  }
}
